{
    "name": "object-encoder",
    "private": false,
    "version": "1.0.2",
    "description": "object-encoder supports encoding class instances into plain JavaScript objects that contain the field information required to restore the source type. 对象编码器支持将类实例编码为包含还原源类型所需字段信息的普通 JS 对象。",
    "keywords": [
        "object",
        "encode",
        "decode",
        "json",
        "serialization",
        "serialize",
        "deserialize"
    ],
    "homepage": "https://github.com/memo-cn/object-encoder/tree/main#readme",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "rimraf ./dist/* && rollup -c --configPlugin rollup-plugin-typescript2",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@babel/core": "^7.28.5",
        "@babel/preset-env": "^7.28.5",
        "@rollup/plugin-babel": "^6.1.0",
        "@rollup/plugin-commonjs": "^29.0.0",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^16.0.3",
        "@rollup/plugin-replace": "^6.0.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@types/node": "^24.10.1",
        "prettier": "^3.7.4",
        "rimraf": "^6.1.2",
        "rollup": "^4.53.3",
        "rollup-plugin-dts": "^6.3.0",
        "rollup-plugin-typescript2": "^0.36.0",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
        "vite": "^7.2.6",
        "vite-plugin-dts": "^4.5.4"
    },
    "files": [
        "dist",
        "CHANGELOG.md",
        "LICENSE",
        "package.json",
        "README.md",
        "README.zh-CN.md"
    ],
    "author": {
        "name": "memo",
        "email": "i@memo.cool"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:memo-cn/object-encoder.git"
    },
    "bugs": {
        "email": "i@memo.cool",
        "url": "https://github.com/memo-cn/object-encoder/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT"
}
